{
    "manifest_version": 3,
    "name": "ChatGPT Continue Button",
    "version": "1.0.0",
    "description": "Add a continue button to ChatGPT",
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://chat.openai.com/chat/*"],
            "js": ["content.js"]
        }
    ]
}
